<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:id="@id/matched_frame_layout" android:background="@color/dialog_matched_background" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout android:id="@id/matched_relative_layout" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <ImageView android:id="@id/imageView_its_a_match" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/margin_med" android:src="@drawable/matchscreen_headlinetext_img" android:layout_above="@id/matched_text" android:layout_centerHorizontal="true" />
