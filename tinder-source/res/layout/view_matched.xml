        </RelativeLayout>
        <View android:id="@id/center_div" android:layout_width="1.0dip" android:layout_height="1.0dip" android:layout_centerInParent="true" />
        <RelativeLayout android:id="@id/linear_match_two" android:background="@drawable/rounded_white_bg" android:layout_width="@dimen/matched_avatar_frame_length" android:layout_height="@dimen/matched_avatar_frame_length" android:layout_marginLeft="@dimen/margin_xsmall" android:layout_toRightOf="@id/center_div" android:layout_above="@id/center_div">
