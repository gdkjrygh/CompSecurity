        <ImageView android:id="@id/superlike_star" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/new_match_star_bottom_margin" android:src="@drawable/new_match_star" android:layout_above="@id/center_div" android:layout_centerHorizontal="true" />
        <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@id/linear_start_chatting" android:background="@drawable/selector_white_button" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/matched_button_margin_top" android:layout_above="@id/linear_find_more_matches" android:layout_alignLeft="@id/linear_match_one" android:layout_alignRight="@id/linear_match_two">
            <ImageView android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/itsamatch_sendmessage_icon" android:scaleType="fitXY" />
