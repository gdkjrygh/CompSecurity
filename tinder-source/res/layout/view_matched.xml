        <com.tinder.views.CustomTextView android:textSize="@dimen/matched_text_size" android:textColor="@color/white" android:gravity="center_horizontal" android:id="@id/matched_text" android:paddingBottom="@dimen/padding_large" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="You and Jennifer have liked each other." android:layout_above="@id/linear_match_one" app:font="light|light_italic" />
        <RelativeLayout android:id="@id/linear_match_one" android:background="@drawable/rounded_white_bg" android:layout_width="@dimen/matched_avatar_frame_length" android:layout_height="@dimen/matched_avatar_frame_length" android:layout_marginRight="@dimen/margin_xsmall" android:layout_toLeftOf="@id/center_div" android:layout_above="@id/center_div">
