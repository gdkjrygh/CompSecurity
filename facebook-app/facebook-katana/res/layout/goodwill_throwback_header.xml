                <com.facebook.resources.ui.FbTextView android:layout_gravity="left" android:id="@id/birthdayTitleText" android:paddingLeft="@dimen/fbui_padding_standard" android:paddingTop="@dimen/goodwill_otd_birthday_padding_top_bot" android:paddingRight="@dimen/fbui_padding_standard" android:paddingBottom="@dimen/goodwill_otd_birthday_padding_mid" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/OtdBirthdayTitleText" />
                <com.facebook.resources.ui.FbTextView android:layout_gravity="left" android:id="@id/birthdaySubtitleText" android:paddingLeft="@dimen/fbui_padding_standard" android:paddingRight="@dimen/fbui_padding_standard" android:paddingBottom="@dimen/goodwill_otd_birthday_padding_top_bot" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/OtdBirthdaySubtitleText" />
            </LinearLayout>
        </com.facebook.ui.compat.fbrelativelayout.FbRelativeLayout>
    </FrameLayout>
    <com.facebook.fbui.widget.megaphone.Megaphone android:id="@id/subscriptionPrompt" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="wrap_content" />
    <com.facebook.goodwill.feed.ui.ThrowbackMegaphone android:id="@id/throwbackSubscriptionPrompt" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="wrap_content" />
</merge>